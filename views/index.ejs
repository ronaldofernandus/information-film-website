<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
  <link rel="preconnect" href="https://fonts.gstatic.com">
  <link href="https://fonts.googleapis.com/css2?family=Assistant:wght@200;300;400;500;600;700;800&family=
Playfair+Display:ital,wght@0,400;0,500;0,600;0,700;0,800;0,900;1,400;1,500;1,600;1,700;1,800;1
,900&display=swap" rel="stylesheet">



  <link rel="stylesheet" href="libraries/bootstrap/css/bootstrap.css">


  <link rel="stylesheet" href="styles/main.css">



<body>
  <div class="container">
    <nav class="row navbar navbar-expand-lg navbar-light bg-white">
      <a href="" class="navbar-brand">
        <h3>Website Film</h3>
      </a>

      <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navigasi">
        <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse" id="navigasi">
        <ul class="navbar-nav ml-auto mr-3">


          <li class="nav-item mx-md-2">
            <a href="/film" class="nav-link active">Film</a></li>
          <a href="/jadwal" class="nav-link">Jadwal</a>
          </li>

          <li class="nav-item mx-md-2">
            <a href="/kategori" class="nav-link">Kategori</a>
          </li>
          <li class="nav-item mx-md-2">
            <a href="/penghubung/add" class="nav-link">Penghubung</a>
          </li>
        </ul>
      </div>
    </nav>
  </div>
  <header class="text-center">
    <h1> Selamat Datang
      <br>
      Di Website Kami
    </h1>
  </header>
  <main>
    <section class="section-popular" id="popular">
        <div class="container">
            <div class="row">
                <div class="col text-center section-popular-heading">
<h2>
    Film Populer
</h2>
<p>
    Berikut daftar film yang sedang populer
</p>
                </div>
            </div>
        </div>

    </section>
    <section class="section-popular-content" id="popularcontent">
      <div class="container">
          <div class="section-popular-travel row justify-content-center">
            <% tempPenghubungs.forEach((tempPenghubung,index) => { %>
              <div class="col-sm-6 col-md-4 col-lg-3">
                  <div class="card-travel text-center d-flex-column"
                  style="background-image: url('<%= tempPenghubung.film.image %>');">
                      <div class="travel-country"> <%= tempPenghubung.film.namaFilm %></div>
                      <div class="travel-button mt-auto">
                          <a href="/detail/<%= tempPenghubung.film.id %>" class="btn btn-travel-detail px-4">
                              View Details
                          </a>
                      </div>
                  </div>
              </div> 
              <% }) %>
          </div>
      </div>
  </section>

  


</main>










  <script src="libraries/jquery/jquery-3.6.0.min.js"></script>


  <script src="libraries/bootstrap/js/bootstrap.js"></script>
  <script src="libraries/retina/retina.min.js"></script>

</body>

</html>